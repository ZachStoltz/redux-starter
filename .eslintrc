{
    "rules": {
        "no-undef": 0,
        no-unused-vars: 1,
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "react/no-multi-comp": 1,
        "react/prefer-es6-class": 1,
        "react/prop-types": 1
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ]
}
